{
  "manifest_version": 2,
  "name": "BayesNews",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.1.1.slim.min.js", "content.js"]
  }
],
  "permissions": [
  "tabs", "activeTab"
],
   "browser_action": {
          "default_popup": "popup.html"        // optional
        }
}
